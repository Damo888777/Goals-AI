{
  "extends": "expo-module-scripts/tsconfig.base",
  "compilerOptions": {
    "outDir": "./build",
    "rootDir": "./src",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es2018"
  },
  "include": ["./src"],
  "exclude": ["**/__tests__/**/*", "**/__mocks__/**/*"]
}
